         android:layout_height="@dimen/setting_bg_item_height" 
         android:layout_marginTop="@dimen/setting_bg_content_margintop" 
         android:layout_marginLeft="0dip"
    ></com.soniq.tvmarket.ui.widget.MetroItemView>
    
