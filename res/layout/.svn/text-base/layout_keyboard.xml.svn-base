<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    <TextView android:id="@+id/char_a"
        android:background="@drawable/btn_search_char_selector"
        android:layout_width="@dimen/search_char_view_width"
        android:layout_height="@dimen/search_char_view_height"
        android:text="A"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/search_char_text_size"
        />
    <TextView android:id="@+id/char_b"
        android:background="@drawable/btn_search_char_selector"
        android:layout_width="@dimen/search_char_view_width"
        android:layout_height="@dimen/search_char_view_height"
        android:text="B"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/search_char_text_size"
        android:layout_alignTop="@+id/char_a"
        android:layout_toRightOf="@+id/char_a"
        android:layout_marginLeft="@dimen/search_char_view_space"
        />
    <TextView android:id="@+id/char_c"
        android:background="@drawable/btn_search_char_selector"
        android:layout_width="@dimen/search_char_view_width"
        android:layout_height="@dimen/search_char_view_height"
        android:text="C"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/search_char_text_size"
        android:layout_alignTop="@+id/char_a"
        android:layout_toRightOf="@+id/char_b"
        android:layout_marginLeft="@dimen/search_char_view_space"
        />
    <TextView android:id="@+id/char_d"
        android:background="@drawable/btn_search_char_selector"
        android:layout_width="@dimen/search_char_view_width"
        android:layout_height="@dimen/search_char_view_height"
        android:text="D"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/search_char_text_size"
        android:layout_alignTop="@+id/char_a"
        android:layout_toRightOf="@+id/char_c"
        android:layout_marginLeft="@dimen/search_char_view_space"
        />
    <TextView android:id="@+id/char_e"
        android:background="@drawable/btn_search_char_selector"
        android:layout_width="@dimen/search_char_view_width"
        android:layout_height="@dimen/search_char_view_height"
        android:text="E"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/search_char_text_size"
        android:layout_alignTop="@+id/char_a"
        android:layout_toRightOf="@+id/char_d"
        android:layout_marginLeft="@dimen/search_char_view_space"
        />
    <TextView android:id="@+id/char_f"
        android:background="@drawable/btn_search_char_selector"
        android:layout_width="@dimen/search_char_view_width"
        android:layout_height="@dimen/search_char_view_height"
        android:text="F"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/search_char_text_size"
        android:layout_alignTop="@+id/char_a"
        android:layout_toRightOf="@+id/char_e"
        android:layout_marginLeft="@dimen/search_char_view_space"
        />

    
    <TextView android:id="@+id/char_g"
        android:background="@drawable/btn_search_char_selector"
        android:layout_width="@dimen/search_char_view_width"
        android:layout_height="@dimen/search_char_view_height"
        android:text="G"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/search_char_text_size"
        android:layout_marginTop="@dimen/search_char_view_space"
        android:layout_below="@+id/char_a"
        android:layout_alignLeft="@+id/char_a"
        />
    <TextView android:id="@+id/char_h"
        android:background="@drawable/btn_search_char_selector"
        android:layout_width="@dimen/search_char_view_width"
        android:layout_height="@dimen/search_char_view_height"
        android:text="H"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/search_char_text_size"
        android:layout_alignTop="@+id/char_g"
        android:layout_toRightOf="@+id/char_g"
        android:layout_marginLeft="@dimen/search_char_view_space"
        />
    <TextView android:id="@+id/char_i"
        android:background="@drawable/btn_search_char_selector"
        android:layout_width="@dimen/search_char_view_width"
        android:layout_height="@dimen/search_char_view_height"
        android:text="I"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/search_char_text_size"
        android:layout_alignTop="@+id/char_g"
        android:layout_toRightOf="@+id/char_h"
        android:layout_marginLeft="@dimen/search_char_view_space"
        />
    <TextView android:id="@+id/char_j"
        android:background="@drawable/btn_search_char_selector"
        android:layout_width="@dimen/search_char_view_width"
        android:layout_height="@dimen/search_char_view_height"
        android:text="J"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/search_char_text_size"
        android:layout_alignTop="@+id/char_g"
        android:layout_toRightOf="@+id/char_i"
        android:layout_marginLeft="@dimen/search_char_view_space"
        />
    <TextView android:id="@+id/char_k"
        android:background="@drawable/btn_search_char_selector"
        android:layout_width="@dimen/search_char_view_width"
        android:layout_height="@dimen/search_char_view_height"
        android:text="K"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/search_char_text_size"
        android:layout_alignTop="@+id/char_g"
        android:layout_toRightOf="@+id/char_j"
        android:layout_marginLeft="@dimen/search_char_view_space"
        />
    <TextView android:id="@+id/char_l"
        android:background="@drawable/btn_search_char_selector"
        android:layout_width="@dimen/search_char_view_width"
        android:layout_height="@dimen/search_char_view_height"
        android:text="L"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/search_char_text_size"
        android:layout_alignTop="@+id/char_g"
        android:layout_toRightOf="@+id/char_k"
        android:layout_marginLeft="@dimen/search_char_view_space"
        />
    
</RelativeLayout>
